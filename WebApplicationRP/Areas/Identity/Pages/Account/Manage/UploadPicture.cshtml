@page
@model WebApplicationRP.Areas.Identity.Pages.Account.Manage.UploadPictureModel
@{
    ViewData["Title"] = "Profile Picture";
    ViewData["ActivePage"] = ManageNavPages.ChangePicture;
}
<h4>@ViewData["Title"]</h4>
<div class="form-group">
    <form  method="post" enctype="multipart/form-data">
        <label asp-for="Input.Image"></label>
        <input type="file" asp-for="Input.Image" class="form-control" />
        <span asp-validation-for="Input.Image" class="text-danger"></span>
        <img src="#" alt="Alternate Text" />
        <button asp-action="UploadPicture" type="submit" class="btn btn-primary">Save</button>
    </form>
</div>